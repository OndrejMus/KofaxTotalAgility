// https://stackoverflow.com/questions/2805330/opening-pdf-string-in-new-window-with-javascript

let pdfWindow = window.open("")

pdfWindow.document.write(
   "<iframe width='100%' height='100%' border='none' overflow='hidden' frameborder='0' margin='0' padding='0' position='fixed' left'0' right'0' bottom'0' top'0px' src='data:application/pdf;base64, " + encodeURI(thisForm.formVariables.getFormVariableByName("DocumentInBase64").getVariableValue()) + "'></iframe>")

setTimeout(function(){ pdfWindow.stop() }, 1000);
